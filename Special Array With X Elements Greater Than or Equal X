int specialArray(int* nums, int numsSize) 
{
    for(int k=0;k<=numsSize;k++)
    {
        int count=0;
        for(int i=0;i<numsSize;i++)
        {
            if(nums[i]>=k)
            {
                count++;
            }
        }
        if(count==k)
        {
            return k;
        }
    }
    return -1;
    
}
